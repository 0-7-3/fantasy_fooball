<div class="panel-body">
  <div class="dataTable_wrapper">
    <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
      <thead>
          <tr>
              <th>All Users</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th class="text-center">Actions</th>
              
          </tr>
      </thead>
      <tbody>

        <% @admin_players.each_with_index do |palyer,index| %>

          <tr class="odd gradeX">
            <td><%= index+1 %></td>
            <td><%= palyer.first_name %></td>
             <td><%= palyer.last_name %></td>
            <td class="text-center">
                <span>
                <%= link_to '', edit_admin_player_path(palyer) ,class: "edit fa fa-edit icon_link with-tip no-text", title: "edit"%>&nbsp; &nbsp;
                </span>
                <span>
               
                <%= link_to '',  admin_player_path(palyer), method: :delete, data: { confirm: 'Are you sure?' } ,class: "delete fa fa-trash icon_link with-tip no-text" , title: "delete" %>
                    
                </span>
            </td>
          </tr>
        <%end%>                                                                             
      </tbody>
    </table>
  </div>
</div>