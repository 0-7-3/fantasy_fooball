<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
       <%= params[:action]== 'new' ? 'New ' : 'Edit'%> User
     </div>
     <div class="panel-body">
      <div class="row">
        <%= nested_form_for [:admin, @admin_team] do |f| %>
        <div class="col-md-6">

          <div class="form-group">
            <%=f.label :name%>
            <%=f.text_field :name ,class: 'form-control', placeholder: 'Enter Name', required: 'true'%>
          </div>
          <div class="form-group">
          <%= f.label :players %>
            <%= f.select :player_ids, options_from_collection_for_select(Player.all, :id,:first_name,f.object.player_ids),{}, :multiple => true ,:class=> 'select' %>
          </div>
          <%= f.submit 'Submit Button', class: "btn btn-default" %>
          <button type="reset" class="btn btn-default">Reset Button</button>
          <a  href="/admin/users" class="btn btn-default">Back</a>
        </div>

        <%end%>
      </div>
    </div>        
  </div>
</div>    
</div>

